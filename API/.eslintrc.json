{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "consistent-return":"off",
        "no-shadow":"off",
        "no-unused-vars":"off",
        "array-callback-return":"off"
    }
}
